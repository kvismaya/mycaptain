a=int(input("Enter the no of terms to be printed"))
f=0                                        
s=1                                         
if a<=0:
    print("The requested series is",f)
else:
    print(f,s,end=" ")
    for x in range(2,a):
        b=f+s                           
        print(b,end=" ")
        f=s
        s=b
